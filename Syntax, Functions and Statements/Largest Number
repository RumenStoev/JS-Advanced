function langestNumber(...params) {
     return `The langest number is ${params.sort((a,b) => a -b).pop()}`
}

console.log(langestNumber(1,2,3,4,5,6))



//Another solution!!!


function langestNumber(...params) {
return `The langest number is ${Math.max(...params)}`
}
console.log(langestNumber(1,78,29,0,-10,100,654))
