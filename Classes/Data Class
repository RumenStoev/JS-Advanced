class Request {
    constructor(method,url,version,message,response,fulfilled) {
     this.method = method;
     this.url = url;
     this.message = message;
     this.response = undefined
     this.fulfilled = false;

    }

}


console.log(new Request("GET","http://google.com","HTTP/1.1",""))

->Request {
  method: 'GET',
  url: 'http://google.com',
  message: '',
  response: undefined,
  fulfilled: false
}
